Bit-1:

#include<stdio.h> 
int main() {     
  void v = 0;      
  printf("%d", v);     
  return 0;
}

Bit-2:

#include<stdio.h> 
int main() {    
 int x=4, y, z;     
 y = --x;     
 z = x--;     
 printf("%d, %d, %d\n", x, y, z);     
 return 0; 
}

Bit-3:

#include <stdio.h>
int main(){
  int x=3, y=4, z=4;
    printf("ans=%d\n",z>=y>=x ?10:20);
  return 0;
}

Bit-4:

#include<stdio.h>  
int main() {      
  int i=-3, j=2, k=0, m;      
  m = ++i && ++j || ++k;      
  printf("%d, %d, %d, %d\n", i, j, k, m);      
  return 0;  
}

Bit-5:

#include <stdio.h>
int main() {
  int i;
  for(i=-1;i<=10;i++) {
    if(i<5)
      continue;
    else
      break;
    printf("Once");
  } 
  return 0;  
}

