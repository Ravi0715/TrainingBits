Bit-7:

#include<stdio.h> 
int main() {    
  int i = 1;     
  switch(i) {    
    printf("This is c program.");   
    case 1:
    printf("Case1");   
    break;      
    case 2:    
    printf("Case2");  
    break; 
  }
  return 0; 
}

Bit-8:

#include<stdio.h>  
int main()  {     
  int P = 10;      
  switch(P) {        
    case 10:         
    printf("Case 1");           
    case 20:        
    printf("Case 2");        
    break;          
    case P:         
    printf("Case 2"); 
    break;    
  }    
  return 0;
}

Bit-9:

#include<stdio.h>  
int main() {    
  int i=1;   
  while() {     
    printf("%d\n", i++);  
    if(i>10){       
      break; 
    }  
  }   
  return 0; 
}

